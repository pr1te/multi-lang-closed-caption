{
  "name": "multi-lang-closed-caption",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:pr1te/multi-lang-closed-caption.git",
  "author": "Jugkapong Pooban <j.pooban@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf ./dist && yarn build && node ./dist/index.js",
    "lint": "eslint --ext .ts ./src ./index.ts --fix",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./index.ts"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.2",
    "lodash": "^4.17.21",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
